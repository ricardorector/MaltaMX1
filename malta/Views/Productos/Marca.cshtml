@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    string especie = ViewBag.especie;
}


@switch (especie)
{
    case "todos":
        @Html.Partial("MenuProductos")
        break;
    case "pecuarios":
        @Html.Partial("Pecuarios")
        break;
    case "mascotas":
        @Html.Partial("Mascotas")
        break;
    case "acuicultura":
        @Html.Partial("Acuicultura")
        break;
}


    </div>
</section>
<script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit"
        async defer>
</script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script type="text/javascript">
    
    var verifyCallback = function (response) {
        $('#validar').show();
    };

    var onloadCallback = function () {
        grecaptcha.render('captcha', {
            'sitekey': '6LcZkCAUAAAAAJc-xi735cVyQFPxmB7pbDt0vmk6',
            'callback': verifyCallback,
            'theme': 'dark'
        });
    };
  
    
    $('#validar').click(function () {
        
        $('#error').html("");
        var validar = fnValidar();
        console.log(validar);
        if (validar == '') {
            $('#validar').hide();
            $('#enviar').show();

        } else {
            
            $('#error').html(validar);
            $('input[name=first_name]').focus();
        }
    });
    
    function fnValidar() {
        var mensaje = "";
        console.log("validar2");
        if ($("input[name=first_name]").val() == '') { mensaje += "* indica tu nombre<br>"; }
        if ($("input[name=last_name]").val() == '') { mensaje += "* indica tus apellidos<br>"; }
        if ($("input[name=mobile]").val() == '') { mensaje += "* indica tu celular<br>"; }
        if ($("input[name=email]").val() == '') { mensaje += "* indica tu email<br>"; }
        if ($("#00N3600000HVTwW").val() == '') { mensaje += "* indica tus comentarios<br>"; }

        if ($("select[name=state_code]").val() == '') { mensaje += "* selecciona un estado<br>"; }
        if ($("select[name=00N3600000RBPAc]").val() == '') { mensaje += "* selecciona un medio de contacto<br>"; }
        if ($("select[name=00N3600000HVACf]").val() == '') { mensaje += "* selecciona un tipo de prospecto<br>"; }
        return mensaje;

    }
    $(function () {
        $('#enviar').hide();
        $('#validar').hide();
        
        
        var ROOT = '@Url.Content("~/Content/")';
        $('#template-header').parallax({ imageSrc: ROOT + 'img/bg-templete.jpg' });
        $('#nosotros-header').parallax({ imageSrc: ROOT + 'img/bg-nosotros.jpg' });
        $('#nosotros-identidad').parallax({ imageSrc: ROOT + 'img/bg-nosotros-historia.jpg' });
        $('#nosotros-historia').parallax({ imageSrc: ROOT + 'img/bg-nosotros-neovia.jpg' });
        $('#template-header').parallax({ imageSrc: ROOT + 'img/bg-templete.jpg' });
    });
</script>
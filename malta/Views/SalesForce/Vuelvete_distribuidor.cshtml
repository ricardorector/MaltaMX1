            <form action="https://webto.salesforce.com/servlet/servlet.WebToLead?encoding=UTF-8" method="POST">

                <input type=hidden name="oid" value="00D36000000p9u5">
                <input type=hidden name="retURL" value="http://www.maltacleyton.com.mx">
                <input type=hidden id="lead_source" name="lead_source" value="Website maltaCleyton">
                <input type=hidden id="country_code" name="country_code" value="MX">
                <input type=hidden id="00N3600000OlRDO" name="00N3600000OlRDO" value="PEC">

                <!--  ----------------------------------------------------------------------  -->
                <!--  NOTA: Estos campos son elementos de depuración opcionales. Elimine      -->
                <!--  los comentarios de estas líneas si desea realizar una prueba en el      -->
